<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Elector" table="elector">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="firstName" type="string" column="First_name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lastName" type="string" column="Last_name" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="language" type="string" column="Language" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="natnum" type="integer" column="NatNum" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="method" type="boolean" column="Method" nullable="true"/>
    <many-to-many field="ballot" target-entity="Ballot" inversed-by="elector" fetch="LAZY">
      <join-table name="elector_has_ballot">
        <join-columns>
          <join-column name="Elector_id" referenced-column-name="id"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="Ballot_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
