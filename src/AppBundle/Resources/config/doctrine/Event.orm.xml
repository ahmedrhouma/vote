<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Event" table="event">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="language" type="string" column="Language" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="title" type="string" column="Title" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="Description" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="company" type="string" column="Company" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="date" type="date" column="Date" nullable="true"/>
  </entity>
</doctrine-mapping>
